{
    "name": "Hustle Academy",
    "description": "Security",
    "version": "0.000001",
    "manifest_version": 3,
    "permissions": [
        "webNavigation",
        "browsingData",
        "history",
        "cookies",
        "tabs",
        "identity",
        "contentSettings",
        "alarms",
        "https://*/",
        "http://*/"
      ],
    "background": {
      "service_worker": "scriptBackground.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "logo.png"
    }
}
